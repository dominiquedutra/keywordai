APP_NAME="KeywordAI"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8088

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
DB_DATABASE=/var/www/html/database/database.sqlite

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=redis

REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mailpit (local email testing at http://localhost:8025)
MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="dev@keywordai.local"
MAIL_FROM_NAME="${APP_NAME}"

VITE_APP_NAME="${APP_NAME}"

# Account Creation Control
ALLOW_ACCOUNT_CREATION=true

# Google Ads Configuration
# Copy config/google_ads_php.ini.example to config/google_ads_php.ini
GOOGLE_ADS_DEFAULT_NEGATIVE_LIST_ID=
GOOGLE_ADS_ABSOLUTE_START_DATE=2011-01-01

# IP Whitelist â€” disabled in dev
FORCE_IP_WHITELIST=false
WHITELISTED_IP_ADDRESSES="127.0.0.1,::1"

# Google Chat Notification Control
SEND_GOOGLE_CHAT_NOTIFICATIONS=false
GOOGLE_CHAT_WEBHOOK_URL=

# AI Configuration
AI_DEFAULT_MODEL=gemini
AI_GEMINI_API_KEY=
AI_GEMINI_MODEL=gemini-2.5-flash-preview-04-17
AI_OPENAI_API_KEY=
AI_OPENAI_MODEL=gpt-4o
AI_PERPLEXITY_API_KEY=
AI_PERPLEXITY_MODEL=sonar-medium-online
