APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# Account Creation Control
ALLOW_ACCOUNT_CREATION=true

GOOGLE_ADS_DEFAULT_NEGATIVE_LIST_ID=
GOOGLE_ADS_ABSOLUTE_START_DATE=2011-01-01

# IP Whitelist Configuration
FORCE_IP_WHITELIST=false
WHITELISTED_IP_ADDRESSES="127.0.0.1,::1" # Comma-separated list of allowed IPs

# Google Chat Notification Control
SEND_GOOGLE_CHAT_NOTIFICATIONS=true
GOOGLE_CHAT_WEBHOOK_URL=

# AI Configuration
AI_DEFAULT_MODEL=gemini
AI_GEMINI_API_KEY=
AI_GEMINI_MODEL=gemini-2.5-flash-preview-04-17
AI_OPENAI_API_KEY=
AI_OPENAI_MODEL=gpt-4o
AI_OPENROUTER_API_KEY=
AI_OPENROUTER_MODEL=google/gemini-2.0-flash-001
