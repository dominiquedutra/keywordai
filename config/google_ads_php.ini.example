[GOOGLE_ADS]
; Required Google Ads API configuration
; Get these from: https://developers.google.com/google-ads/api/docs/first-call/overview

; Developer Token - Request from Google Ads API Center
developerToken = "YOUR_DEVELOPER_TOKEN_HERE"

; Manager Account ID (MCC) - The account that owns the API access
; Remove dashes, e.g., 123-456-7890 becomes 1234567890
loginCustomerId = "YOUR_MANAGER_ACCOUNT_ID"

; Client Account ID - The account you want to query
; This is the actual ad account, not the MCC
clientCustomerId = "YOUR_CLIENT_ACCOUNT_ID"

[OAUTH2]
; OAuth2 credentials from Google Cloud Console
; Create these at: https://console.cloud.google.com/apis/credentials

clientId = "YOUR_OAUTH_CLIENT_ID.apps.googleusercontent.com"
clientSecret = "YOUR_OAUTH_CLIENT_SECRET"

; Refresh Token - Get this by running: php scripts/get_refresh_token.php
; This token allows API access without manual login each time
refreshToken = "YOUR_REFRESH_TOKEN_HERE"

[LOGGING]
; Optional: Enable API logging for debugging
; logFilePath = "storage/logs/google_ads_api.log"
; logLevel = "INFO"

[CONNECTION]
; Optional: Use REST instead of gRPC if gRPC is not available
; transport = "rest"
