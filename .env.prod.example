APP_NAME="KeywordAI"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=http://localhost:8080

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=warning

DB_CONNECTION=sqlite
DB_DATABASE=/var/www/html/database/database.sqlite

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

VITE_APP_NAME="${APP_NAME}"

# Account Creation Control
# Set to false to disable registration after creating your admin user
ALLOW_ACCOUNT_CREATION=true

# Google Ads Configuration
# These are configured via config/google_ads_php.ini
# Copy config/google_ads_php.ini.example to config/google_ads_php.ini
GOOGLE_ADS_DEFAULT_NEGATIVE_LIST_ID=
GOOGLE_ADS_ABSOLUTE_START_DATE=2011-01-01

# IP Whitelist Configuration
FORCE_IP_WHITELIST=false
WHITELISTED_IP_ADDRESSES="127.0.0.1,::1"

# Google Chat Notification Control
SEND_GOOGLE_CHAT_NOTIFICATIONS=true
GOOGLE_CHAT_WEBHOOK_URL=

# AI Configuration
# Uncomment and fill in the APIs you want to use
AI_DEFAULT_MODEL=gemini
# AI_GEMINI_API_KEY=your_gemini_api_key_here
# AI_GEMINI_MODEL=gemini-2.5-flash
# AI_OPENAI_API_KEY=your_openai_api_key_here
# AI_OPENAI_MODEL=gpt-4o
# AI_OPENROUTER_API_KEY=your_openrouter_api_key_here
# AI_OPENROUTER_MODEL=google/gemini-2.0-flash-001
